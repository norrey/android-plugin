<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ke.co.company.samplechatbot">

    <uses-permission android:name="android.permission.INTERNET"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="talklift_chat_c_color" android:value="@string/talklift_chat_c_color" />
        <meta-data android:name="talklift_chat_base_color" android:value="@string/talklift_chat_base_color" />
        <meta-data android:name="talklift_chat_text_color" android:value="@string/talklift_chat_text_color" />
        <meta-data android:name="talklift_chat_org_id" android:value="@string/talklift_chat_org_id" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>